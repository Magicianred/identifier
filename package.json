{
  "name": "identifier",
  "version": "1.0.0",
  "description": "A CRUD application with Graphql using DDD and Clean Architecture",
  "author": "Thiago Gomes",
  "license": "MIT",
  "repository": "https://github.com/th14god3v/identifier",
  "homepage": "https://github.com/th14g0d3v/th14g0d3v",
  "scripts": {
    "ts:dev": "ts-node-dev --watch ./src --respawn --transpile-only src/main/server.ts",
    "ts:debug": "ts-node-dev --watch ./src --inspect=0.0.0.0:9222 --debug src/main/server.ts",
    "js:dev": "rimraf dist && tsc && node dist/main/server.js",
    "js:debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
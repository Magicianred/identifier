{
  "name": "identifier",
  "version": "1.0.0",
  "description": "A CRUD application with Graphql using DDD and Clean Architecture",
  "author": "Thiago Gomes",
  "license": "MIT",
  "repository": "https://github.com/th14god3v/identifier",
  "homepage": "https://github.com/th14g0d3v/th14g0d3v",
  "scripts": {
    "ts:dev": "ts-node-dev --watch ./src --respawn --transpile-only src/main/server.ts",
    "ts:deb": "ts-node-dev --watch ./src --inspect=0.0.0.0:9222 --debug src/main/server.ts",
    "js:dev": "rimraf dist && tsc && node dist/main/server.js",
    "js:deb": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.10",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.21.0",
    "argon2": "^0.27.1",
    "bson": "^4.2.3",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.4"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
